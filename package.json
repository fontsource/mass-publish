{
	"name": "mass-publish",
	"version": "2.0.0",
	"packageManager": "pnpm@7.9.0",
	"description": "A barebones NPM bulk publisher.",
	"author": "Ayuhito <declininglotus@gmail.com>",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		"types": "./dist/index.d.ts",
		"require": "./dist/index.js",
		"import": "./dist/index.mjs"
	},
	"bin": {
		"gfm": "./dist/cli.mjs"
	},
	"license": "MIT",
	"scripts": {
		"build": "rimraf ./dist && pkgroll --target=node14 && tsx ./scripts/postbuild.ts",
		"dev": "pkgroll --target=node14 --watch",
		"lint": "eslint **/*.ts --fix",
		"test": "vitest"
	},
	"dependencies": {
		"@npmcli/package-json": "^2.0.0",
		"cac": "^6.7.12",
		"consola": "^2.15.3",
		"defu": "^6.1.0",
		"dotenv": "^16.0.1",
		"execa": "^6.1.0",
		"json-stringify-pretty-compact": "^4.0.0",
		"latest-version": "^7.0.0",
		"libnpmpublish": "^6.0.4",
		"p-queue": "^7.3.0",
		"pacote": "^13.6.1",
		"parse-git-config": "^3.0.0",
		"pathe": "^0.3.3",
		"picocolors": "^1.0.0",
		"prompts": "^2.4.2",
		"semver": "^7.3.7"
	},
	"devDependencies": {
		"@ayuhito/eslint-config": "^0.2.4",
		"@types/libnpmpublish": "^4.0.3",
		"@types/node": "^18.6.4",
		"@types/pacote": "^11.1.5",
		"@types/parse-git-config": "^3.0.1",
		"@types/prompts": "^2.0.14",
		"@types/semver": "^7.3.10",
		"c8": "^7.12.0",
		"eslint": "^8.21.0",
		"fast-glob": "^3.2.11",
		"magic-string": "^0.26.2",
		"pkgroll": "^1.4.0",
		"prettier": "^2.7.1",
		"rimraf": "^3.0.2",
		"tsx": "^3.8.0",
		"typescript": "^4.7.4",
		"vitest": "^0.21.0"
	},
	"repository": "https://github.com/fontsource/mass-publish.git"
}
